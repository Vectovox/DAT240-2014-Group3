<?xml version="1.0" encoding="ASCII"?>
<wfe:Workflow
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:wfe="http://wfe/1.0"
    xsi:schemaLocation="http://wfe/1.0 model/wfe.ecore"
    name="dieWorkflow">
  <contains_arcs
      source="start"
      target="confirm"
      name="startToConfirm"/>
  <contains_arcs
      source="start"
      target="wait"
      name="startToSleep"/>
  <contains_arcs
      source="synch"
      target="end"
      name="synchToEnd"/>
  <contains_arcs
      source="wait"
      target="synch"
      name="sleepToSynch"/>
  <contains_arcs
      source="confirm"
      target="synch"
      name="confirmToSynch"/>
  <contains_arcs
      source="loopconfirm"
      target="confirm"
      name="loopconfirmToConfirm"/>
  <contains_arcs
      source="confirm"
      target="loopconfirm"
      name="confirmToLoopconfirm"/>
  <contains_nodes
      xsi:type="wfe:StartNode"
      name="start"
      outgoing="startToConfirm startToSleep"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:Sleep"
      name="wait"
      outgoing="sleepToSynch"
      incoming="startToSleep"
      in="/">
    <seconds>10.0</seconds>
  </contains_nodes>
  <contains_nodes
      xsi:type="wfe:Confirm"
      name="confirm"
      outgoing="confirmToSynch confirmToLoopconfirm"
      incoming="startToConfirm loopconfirmToConfirm"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:Synchbar"
      name="synch"
      outgoing="synchToEnd"
      incoming="sleepToSynch confirmToSynch"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:EndNode"
      name="end"
      incoming="synchToEnd"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:Confirm"
      name="loopconfirm"
      outgoing="loopconfirmToConfirm"
      incoming="confirmToLoopconfirm"
      in="/"/>
</wfe:Workflow>
