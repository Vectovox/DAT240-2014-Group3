<?xml version="1.0" encoding="ASCII"?>
<wfe:Workflow
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:wfe="http://wfe/1.0"
    xsi:schemaLocation="http://wfe/1.0 model/wfe.ecore"
    name="dieWorkflow">
  <contains_arcs
      source="start"
      target="confirm"
      name="startToConfirm"/>
  <contains_arcs
      source="start"
      target="wait"
      name="startToSleep"/>
  <contains_arcs
      source="synch"
      target="end"
      name="synchToEnd"/>
  <contains_arcs
      source="wait"
      target="synch"
      name="sleepToSynch"/>
  <contains_arcs
      source="confirm"
      target="setVariable"
      name="confirmToSetVariable"/>
  <contains_arcs
      source="confirm"
      target="gate"
      name="confirmToGate"/>
  <contains_arcs
      source="gate"
      target="synch"
      name="gateToSynch"/>
  <contains_arcs
      source="setVariable"
      target="confirm"
      name="setVariableToConfirm"/>
  <contains_nodes
      xsi:type="wfe:StartNode"
      name="start"
      outgoing="startToConfirm startToSleep"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:Sleep"
      name="wait"
      outgoing="sleepToSynch"
      incoming="startToSleep"
      in="/"
      seconds="10.0"/>
  <contains_nodes
      xsi:type="wfe:Confirm"
      name="confirm"
      outgoing="confirmToSetVariable confirmToGate"
      incoming="startToConfirm setVariableToConfirm"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:Synchbar"
      name="synch"
      outgoing="synchToEnd"
      incoming="sleepToSynch gateToSynch"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:EndNode"
      name="end"
      incoming="synchToEnd"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:Gate"
      name="gate"
      outgoing="gateToSynch"
      incoming="confirmToGate"
      in="/"
      condition="gateVariable"/>
  <contains_nodes
      xsi:type="wfe:SetVariable"
      name="setVariable"
      outgoing="setVariableToConfirm"
      incoming="confirmToSetVariable"
      in="/"
      description=""
      sets_variable="gateVariable"
      evaluated_expression="true"/>
  <contains_expressions
      xsi:type="wfe:Variable"
      name="gateVariable"/>
  <contains_expressions
      xsi:type="wfe:True"
      name="true"/>
</wfe:Workflow>
