<?xml version="1.0" encoding="ASCII"?>
<wfe:Workflow
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:wfe="http://wfe/1.0">
  <contains_arcs
      source="//@contains_nodes.0"
      target="//@contains_nodes.2"
      name="start > wait"/>
  <contains_arcs
      source="//@contains_nodes.2"
      target="//@contains_nodes.1"
      name="wait > end"/>
  <contains_arcs
      source="//@contains_nodes.0"
      target="//@contains_nodes.4"
      name="start  > confirm1"/>
  <contains_arcs
      source="//@contains_nodes.4"
      target="//@contains_nodes.3"
      name="confirm1 > gate1"/>
  <contains_arcs
      source="//@contains_nodes.3"
      target="//@contains_nodes.1"
      name="gate1 > end"/>
  <has_resources
      xsi:type="wfe:Actor"
      name="Employee1"
      description=""
      first_name="Klaus"
      last_name="Stapelfahrer"/>
  <contains_nodes
      xsi:type="wfe:StartNode"
      name="start"
      outgoing="//@contains_arcs.0 //@contains_arcs.2"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:EndNode"
      name="end"
      incoming="//@contains_arcs.1 //@contains_arcs.4"
      in="/"/>
  <contains_nodes
      xsi:type="wfe:Sleep"
      name="wait"
      outgoing="//@contains_arcs.1"
      incoming="//@contains_arcs.0"
      in="/"
      description="">
    <seconds>30.0</seconds>
  </contains_nodes>
  <contains_nodes
      xsi:type="wfe:Gate"
      name="gate1"
      outgoing="//@contains_arcs.4"
      incoming="//@contains_arcs.3"
      in="/"
      description=""
      condition="//@contains_expressions.0"/>
  <contains_nodes
      xsi:type="wfe:Confirm"
      name="confirm1"
      outgoing="//@contains_arcs.3"
      incoming="//@contains_arcs.2"
      in="/"
      has_assigned="//@has_resources.0"/>
  <contains_expressions
      xsi:type="wfe:Variable"
      name="continue"/>
</wfe:Workflow>
